<?php

namespace Valk\Bundle\MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Realmcharacters
 *
 * @ORM\Table(name="realmcharacters", indexes={@ORM\Index(name="acctid", columns={"acctid"})})
 * @ORM\Entity
 */
class Realmcharacters
{
    /**
     * @var boolean
     *
     * @ORM\Column(name="numchars", type="boolean", nullable=false)
     */
    private $numchars;

    /**
     * @var integer
     *
     * @ORM\Column(name="realmid", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     */
    private $realmid;

    /**
     * @var integer
     *
     * @ORM\Column(name="acctid", type="bigint")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     */
    private $acctid;


}
