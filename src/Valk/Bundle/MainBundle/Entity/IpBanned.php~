<?php

namespace Valk\Bundle\MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * IpBanned
 *
 * @ORM\Table(name="ip_banned")
 * @ORM\Entity
 */
class IpBanned
{
    /**
     * @var integer
     *
     * @ORM\Column(name="unbandate", type="bigint", nullable=false)
     */
    private $unbandate;

    /**
     * @var string
     *
     * @ORM\Column(name="bannedby", type="string", length=50, nullable=false)
     */
    private $bannedby;

    /**
     * @var string
     *
     * @ORM\Column(name="banreason", type="string", length=255, nullable=false)
     */
    private $banreason;

    /**
     * @var string
     *
     * @ORM\Column(name="ip", type="string", length=32)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     */
    private $ip;

    /**
     * @var integer
     *
     * @ORM\Column(name="bandate", type="bigint")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     */
    private $bandate;


}
