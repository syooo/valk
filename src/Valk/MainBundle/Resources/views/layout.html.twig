<html  xml:lang="ru-ru" class="ru-ru">
<head xmlns:og="http://ogp.me/ns#" xmlns:fb="http://ogp.me/ns/fb#">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta http-equiv="imagetoolbar" content="false" />

    <link rel="shortcut icon" href="/shop/static/images/meta/favicon.ico" type="image/x-icon" />
    <link rel="icon" href="/shop/static/images/meta/favicon.png" type="image/png" />

    <title>
        {%- block title -%}
        Valkyrie WoW - Classic World of Warcraft Server 1.12.1, 1.12.2
        {%- endblock -%}
    </title>

    {% block css %}

        {% stylesheets filter='cssrewrite'
         %}
            <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

        {% stylesheets filter="compass"
            '@ValkMainBundle/Resources/assets/sass/bootstrap.sass'
        %}
            <link rel="stylesheet" href="{{ asset_url }}"/>
        {% endstylesheets %}

    {% endblock %}

    {% block javascripts %}

        {% javascripts '@ValkMainBundle/Resources/public/js/zepto/dist/zepto.js'
                       '@ValkMainBundle/Resources/public/js/namespace.coffee'
        %}
            <script src="{{ asset_url }}" type="text/javascript"></script>
        {% endjavascripts %}
    {% endblock %}

    <meta property="og:type" content="website" />
    <meta property="og:title" content="Магазин Battle.net" />
    <meta property="og:image" content="https://eu.battle.net/shop/static/images/logos/og-shop.png?v=58-1.2.10" />
    <meta property="og:image" content="https://eu.battle.net/shop/static/images/logos/og-blizzard.png?v=58-1.2.10" />
    <meta property="og:description" content="" />
    <meta property="og:url" content="https://eu.battle.net/shop/en-gb/" />
    <meta property="og:locale" content="ru_RU" />
    <link rel="canonical" href="https://eu.battle.net/shop/en-gb/" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body class="browse-template">


<header class="header">
<div class="header-primary">
<div class="container" style="position: relative;">
<h1 class="heading-3 text-logo">
    <a href="{{ path('valk_main_homepage') }}">
        Valkyrie WoW
        <span class="small">Vanilla 1.12.2</span>
    </a>
</h1>


<div  id="service">
    <ul class="service-bar">
        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
            <li class="service-cell service-welcome">
                Welcome, {{ app.user.username }}
                |  <a href="{{ path('fos_user_security_logout') }}" tabindex="50" accesskey="2">log out</a>
            </li>
        {% else %}
            <li class="service-cell service-welcome">
                <a href="{{ path('fos_user_security_login') }}" >Log in</a> or <a href="{{ path('fos_user_registration_register') }}">Create an Account</a>
            </li>
        {% endif %}

        <li class="service-cell service-shop">
            <a href="#account" class="service-link" data-action="Shop">Account</a>
        </li>
        <li class="service-cell service-shop">
            <a href="{{ attribute(urls, app.request.locale).armory }}" class="service-link" data-action="Shop">Armory</a>
        </li>
        <li class="service-cell service-shop">
            <a href="{{ attribute(urls, app.request.locale).db }}" class="service-link" data-action="Shop">Database</a>
        </li>
        <li class="service-cell service-account">
            <a href="{{ attribute(urls, app.request.locale).forum }}" class="service-link" tabindex="50" accesskey="3" data-action="Account">Boards</a>
        </li>


    </ul>
    <div id="warnings-wrapper">

        <noscript>
            <div id="javascript-warning" class="warning warning-red">
                <div class="warning-inner2">
                    JavaScript must be enabled to use this site.
                </div>
            </div>
        </noscript>
    </div>

</div>


</div>
</div>
<div class="navbar navbar-static-top header-secondary">
    <div class="navbar-inner">
        <div class="container">

            {% import 'ValkMainBundle:blocks:breadcrumbs/breadcrumbs.html.twig' as breadcrumbs %}

            {% block breadcrumbs %}
                {{ breadcrumbs.show() }}
            {% endblock %}


            <div class="server-status pull-right" >
                <span>set realmlist logon.valkyrie-wow.com</span>
                <span class="online-players">420 players online</span>
                <span>rev. 2406</span>
            </div>


        </div>
    </div>
</div>
</header>



<div class="container home">
    {% block content %}

    {% endblock %}
</div>


